{
  "manifest_version": 3,
  "name": "FE control",
  "version": "1.0",
  "description": "tema personalizado para el facturador web",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "app/popup/index.html"
  },
  "background":{
    "service_worker":"app/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://e-vf.softwareintegrado.com/*"
      ],
      "js": [
        "app/content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "app/*"
      ],
      "matches": [
        "https://e-vf.softwareintegrado.com/*"
      ]
    }
  ]
}