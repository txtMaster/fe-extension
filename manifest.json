{
  "manifest_version": 3,
  "name": "FE control",
  "version": "1.0",
  "description": "tema personalizado para el facturador web",

  "content_scripts": [
    {
      "matches": [
        "https://e-vf.softwareintegrado.com/*"
      ],
      "js": ["app/index.js"]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": ["app/*"],
      "matches": ["https://e-vf.softwareintegrado.com/*"]
    }
  ]
}